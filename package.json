{
  "name": "discord-selfbot-ts",
  "version": "1.0.0",
  "description": "A Discord selfbot built on TypeScript.",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc && npm prune --production",
    "start": "node dist/index.js",
    "once": "tsc && npm start",
    "dev": "tsc-watch --onSuccess \"node dist/index.js dev\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/keanuplayz/discord-selfbot-ts.git"
  },
  "author": "Keanu Timmermans",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/keanuplayz/discord-selfbot-ts/issues"
  },
  "homepage": "https://github.com/keanuplayz/discord-selfbot-ts#readme",
  "dependencies": {
    "discord.js": "^11.3.2",
    "inquirer": "^7.3.3"
  },
  "devDependencies": {
    "@types/inquirer": "^7.3.1",
    "@types/node": "^14.14.2",
    "@types/ws": "^7.2.7",
    "prettier": "2.1.2",
    "ts-node": "^9.0.0",
    "tsc-watch": "^4.2.9",
    "typescript": "^4.0.3"
  }
}
